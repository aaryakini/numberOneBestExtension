{
    "manifest_version": 3,
    "name": "Number One Best Work Extension",
    "version": "1.0",
    "icons": {
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html"
      },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ], 
    "permissions": [
        "storage", 
        "alarms", 
        "notifications", 
        "tabs"
      ], 
    "background": {
        "service_worker": "background.js"
    }
  }