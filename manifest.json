{
    "manifest_version": 3,
    "name": "Number One Best Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
      },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ], 
    "permissions": [
        "storage", 
        "alarms", 
        "notifications", 
        "tabs"
      ], 
    "background": {
        "service_worker": "background.js"
    }
  }